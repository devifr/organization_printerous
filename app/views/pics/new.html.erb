<div class="card">
  <div class="card-header">
    <h3> <%= "New PIC on #{@organization.name}" %></h3>
  </div>
  <div class="card-body">
    <%= form_for(@pic, url: organization_pics_path, method: "post", html: {class: "form-horizontal"}) do |f| %>
     <%= render partial:"form", locals: {f: f} %>
    <% end %>
  </div>
</div>
